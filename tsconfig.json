{
  "compilerOptions": {
    "target": "es2019",
    "downlevelIteration": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "outDir": "dist",
    "sourceMap": true,
    "declaration": true,
    "paths": {
      "@/*": ["./lib/*"]
    }
  }
}
